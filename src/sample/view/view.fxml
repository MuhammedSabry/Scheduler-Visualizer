<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="508.0" prefWidth="508.0" spacing="16.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.ViewController">
    <children>
        <ScrollPane hbarPolicy="ALWAYS" maxWidth="1.7976931348623157E308" scaleShape="false" vbarPolicy="NEVER" vmax="0.0">
            <content>
                <HBox fx:id="queue_chart_box" alignment="CENTER">
                </HBox>
            </content>
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
        </ScrollPane>
        <Label fx:id="avgTime" text="Average waiting time =">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Label>
      <Button fx:id="repeat" mnemonicParsing="false" text="Repeat" />
    </children>
    <padding>
        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
    </padding>
</VBox>
